{
  "name": "radiostreamplayerrn",
  "private": true,
  "main": "index.js",
  "version": "0.0.1",
  "scripts": {
    "rn-cli": "node scripts/react-native.js",
    "start:android": "yarn rn-cli run-android",
    "start:windows": "yarn rn-cli run-windows",
    "start:ios": "yarn rn-cli run-ios",
    "start:web": "cross-env platform=web webpack-dev-server --config=web/webpack/dev.js --progress --colors --mode=development",
    "start:rn-dev-server": "yarn rn-cli start --reset-cache",
    "build:web": "cross-env platform=web webpack --config=web/webpack/prod.js --progress --colors --mode=production",
    "test": "jest -c jest/jest.config.js",
    "test:watch": "yarn test --watch",
    "test:debug": "node --inspect-brk node_modules/.bin/jest -c jest/jest.config.js --runInBand",
    "build:types": "tsc --emitDeclarationOnly",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -w",
    "lint": "eslint --config .eslintrc --ext .ts,.tsx src",
    "rnlink": "react-native link"
  },
  "devDependencies": {
    "@babel/core": "7.8.6",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/preset-env": "7.8.6",
    "@react-native-community/cli": "1.11.2",
    "@types/cheerio": "^0.22.14",
    "@types/enzyme": "3.10.5",
    "@types/jest": "25.1.3",
    "@types/react-native-vector-icons": "^6.4.4",
    "@types/react-native-video": "^3.1.5",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "babel-loader": "8.0.6",
    "compression-webpack-plugin": "3.1.0",
    "cross-env": "7.0.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.4",
    "eslint": "6.8.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-jest": "23.8.1",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-reactxp": "0.1.8",
    "fork-ts-checker-webpack-plugin": "4.0.5",
    "html-webpack-plugin": "5.5.0",
    "jest": "25.1.0",
    "metro-react-native-babel-preset": "0.58.0",
    "rnpm-plugin-windows": "0.4.1",
    "typescript": "3.8.3",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "4.2.2"
  },
  "dependencies": {
    "@react-navigation/material-top-tabs": "^5.2.2",
    "@react-navigation/native": "^5.3.0",
    "cheerio": "0.22.0",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-native": "0.59.10",
    "react-native-gesture-handler": "~1.4.0",
    "react-native-music-control": "^0.10.7",
    "react-native-reanimated": "^2.10.0",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-native-tab-view": "2.11.0",
    "react-native-vector-icons": "^6.6.0",
    "react-native-video": "^5.0.2",
    "react-native-windows": "0.59.0-rc.3",
    "reactxp": "2.0.0"
  }
